{{ define "contact" }}
  <div class="contact">
          <div class="contactHead">CONTACT </div><div class="contactLogo">{{ template "inlineLogo" }}</div>
    <form>
      <input class="emailInput" id="emailInput" placeholder="e-mail" name="email" type="email" />
      <textarea class="messageArea" id="messageArea" placeholder="Project Description and Budget"></textarea>
      <div class="sendButt" id="sendButt" onClick="sendMail()">Send</div>
    </form>
  </div>
{{end}}
